CREATE INDEX t_backward_upper_idx ON t_backward_upper USING smol(k);
ANALYZE t_backward_upper;
-- Backward scan with upper bound (<=)
SELECT k FROM t_backward_upper WHERE k <= 500 ORDER BY k DESC LIMIT 10;
